{
  "name": "fourth-step",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "build:test": "NODE_ENV=test next build",
    "dev": "next dev",
    "postinstall": "prisma generate",
    "schema:generate": "prisma generate",
    "schema:push": "prisma db push",
    "schema:generate:test": "dotenv -e .env.test prisma generate",
    "schema:push:test": "dotenv -e .env.test prisma db push",
    "lint": "next lint",
    "start": "next start",
    "start:test": "NODE_ENV=test next start",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "cypress": "dotenv -e .env.test cypress open",
    "cypress:headless": "dotenv -e .env.test cypress run --browser=chrome --headless",
    "e2e": "start-server-and-test 'yarn start:test' http://localhost:3000 'yarn cypress'",
    "e2e:headless": "start-server-and-test 'yarn start:test' http://localhost:3000 'yarn cypress:headless'"
  },
  "dependencies": {
    "@prisma/client": "^4.5.0",
    "@tanstack/react-query": "^4.16.0",
    "@trpc/client": "^10.0.0",
    "@trpc/next": "^10.0.0",
    "@trpc/react-query": "^10.0.0",
    "@trpc/server": "^10.0.0",
    "next": "13.1.1",
    "next-auth": "^4.18.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "superjson": "1.9.1",
    "zod": "^3.18.0"
  },
  "devDependencies": {
    "@testing-library/cypress": "^9.0.0",
    "@types/node": "^18.0.0",
    "@types/prettier": "^2.7.2",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "autoprefixer": "^10.4.7",
    "cypress": "^12.3.0",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.26.0",
    "eslint-config-next": "13.1.1",
    "postcss": "^8.4.14",
    "prettier": "^2.8.1",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "prisma": "^4.5.0",
    "start-server-and-test": "^1.15.2",
    "tailwindcss": "^3.2.0",
    "typescript": "^4.8.4"
  },
  "ct3aMetadata": {
    "initVersion": "6.11.6"
  }
}
